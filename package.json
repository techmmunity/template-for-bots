{
	"name": "maite-discord-bot",
	"version": "1.0.0",
	"description": "Maite Discord Bot",
	"author": "Monetizzer",
	"private": false,
	"license": "Apache-2.0",
	"dependencies": {
		"@techmmunity/utils": "^1.10.1",
		"axios": "^1.1.3",
		"nanoid": "^3.3.4"
	},
	"devDependencies": {
		"@serverless/typescript": "^3.21.0",
		"@techmmunity/eslint-config": "^5.2.4",
		"@types/jest": "^29.0.3",
		"@types/node": "^18.7.23",
		"@types/supertest": "^2.0.12",
		"@types/uuid": "^8.3.3",
		"eslint": "^8.24.0",
		"husky": "^8.0.1",
		"jest": "^29.0.3",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"serverless": "^3.22.0",
		"serverless-webpack": "^5.10.0",
		"supertest": "^6.2.4",
		"terser-webpack-plugin": "^5.3.6",
		"ts-jest": "^29.0.2",
		"ts-loader": "^9.4.1",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.3",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0"
	},
	"scripts": {
		"experiment": "ts-node src/.experiments/index.ts",
		"ts-node": "ts-node",
		"prepare": "husky install",
		"build": "serverless package",
		"build:bot": "npx webpack --config webpack.bot.config.js",
		"start:bot": "node bot/main.js",
		"deploy:production": "NODE_ENV=production env-cmd serverless deploy --stage production",
		"deploy:dev": "NODE_ENV=dev env-cmd serverless deploy --stage dev",
		"deploy:local": "NODE_ENV=local env-cmd serverless deploy --stage local",
		"deploy:production:all": "./scripts/deploy-serverless-production.sh",
		"deploy:dev:all": "./scripts/deploy-serverless-dev.sh",
		"format": "eslint . --fix --quiet",
		"lint": "eslint . --quiet",
		"test": "jest --passWithNoTests",
		"test:cov": "jest --coverage --passWithNoTests",
		"upgrade-dependencies": "yarn upgrade-interactive --latest",
		"husky:pre-commit": "tsc && yarn lint"
	}
}
